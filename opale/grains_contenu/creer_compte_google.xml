								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Cliquer sur le bouton : « Ouvrir » ou : « Open»</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_bouton_open.png"/>
							</op:txtRes>
