							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Remplir le champ : « Ajoutez une légende ». Entrer, par exemple : « Ma photo de profil ».</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_champ_ajouter_legende.png"/>
							</op:txtRes>
						</sp:txtRes>
