						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Cliquer sur le bouton : « Séectionner une photo sur votre ordinateur »</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_bouton_selectionner_photo_ordinateur.png"/>
							</op:txtRes>
