						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Choisir votre photographie de profil</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_boite_dialogue_photo_profil.png"/>
