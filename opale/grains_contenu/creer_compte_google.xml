								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Cadrer l'image à votre goût.</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_cadrage_image_profil.png"/>
							</op:txtRes>
