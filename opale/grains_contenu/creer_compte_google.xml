<?xml version="1.0" encoding="UTF-8"?>
<sc:item xmlns:sc="http://www.utc.fr/ics/scenari/v3/core">
	<op:expUc xmlns:op="utc.fr:ics/opale3" xmlns:sp="http://www.utc.fr/ics/scenari/v3/primitive">
		<op:uM>
			<sp:title>Créer un compte Google</sp:title>
		</op:uM>
		<sp:pb>
			<op:pb>
				<sp:info>
					<op:pbTi/>
					<op:res>
						<sp:txt>
							<op:txt>
								<sc:para xml:space="preserve">Pour créer votre compte Google Blogger vous devez.</sc:para>
							</op:txt>
						</sp:txt>
						<sp:txt>
							<op:txt>
								<sc:para xml:space="preserve">Ouvrir votre navigateur internet.</sc:para>
								<sc:para xml:space="preserve">Dans la barre d'adresse, saisir l'adresse : « <sc:phrase role="url"><op:urlM><sp:url>https://www.blogger.com</sp:url><sp:title>Google Blogger</sp:title></op:urlM>https://www.blogger.com</sc:phrase> »</sc:para>
							</op:txt>
						</sp:txt>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Cliquer sur le bouton : «Créez votre blogue»</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_blogue_bouton_creez_votre_blog.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Cliquer sur le lien hypertexte : « Créer un compte »</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_blogue_lien_creer_compte.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Entrer votre : « Prénom » et : « Nom »</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_nom_prenom.png"/>
							</op:txtRes>
						</sp:txtRes>
					</op:res>
				</sp:info>
				<sp:warning>
					<op:pbTi>
						<sp:title>La saisie du futur nom d'utilisateur</sp:title>
					</op:pbTi>
					<op:res>
						<sp:txt>
							<op:txt>
								<sc:para xml:space="preserve">Entrer le future nom d'utilisateur, de la manière suivante : </sc:para>
								<sc:para xml:space="preserve">Le <sc:inlineStyle role="emp">sigle du cours</sc:inlineStyle> + le <sc:inlineStyle role="emp"><sc:inlineStyle role="emp">trimestre</sc:inlineStyle></sc:inlineStyle><sc:inlineStyle role="emp"> et l'année</sc:inlineStyle> +<sc:inlineStyle role="emp"> initiale</sc:inlineStyle> </sc:para>
								<sc:para xml:space="preserve">Par exemple : pour l'élève portant le nom de <sc:inlineStyle role="emp">William Tremblay</sc:inlineStyle> de la cohorte <sc:inlineStyle role="emp">Automne 2016</sc:inlineStyle> :</sc:para>
								<sc:para xml:space="preserve"><sc:inlineStyle role="emp">DDD9651A16WT</sc:inlineStyle></sc:para>
							</op:txt>
						</sp:txt>
					</op:res>
				</sp:warning>
				<sp:info>
					<op:pbTi/>
					<op:res>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Entrer et confirmer le : « mot de passe » que vous avez préalablement créé.</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_creer_confirmer_mot_passe.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:itemizedList>
											<sc:listItem>
												<sc:para xml:space="preserve">Entrer dans le premier champ votre le : « jour » de votre naissance</sc:para>
											</sc:listItem>
											<sc:listItem>
												<sc:para xml:space="preserve">Sélectionner à l'aide de la liste déroulante le « mois » de votre naissance</sc:para>
											</sc:listItem>
											<sc:listItem>
												<sc:para xml:space="preserve">Entrer le dernier champ l' : « Année »  de votre naissance </sc:para>
											</sc:listItem>
										</sc:itemizedList>
										<sc:para xml:space="preserve">N.B. : il n'est pas nécessaire d'inscrire votre vraie date de naissance.</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_blogue_date_naissance.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Sélectionner dans la liste déroulante votre : « Sexe ».</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_sexe.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Entrer dans le champ votre : « Numéro de téléphone mobile »</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_numero_telephone_mobile.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Entrer dans le champ votre : « adresse e-mail actuelle »</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_adresse_courriel_actuelle.png"/>
							</op:txtRes>
						</sp:txtRes>
					</op:res>
				</sp:info>
				<sp:warning>
					<op:pbTi>
						<sp:title>Quelle adresse courriel saisir ?</sp:title>
					</op:pbTi>
					<op:res>
						<sp:txt>
							<op:txt>
								<sc:para xml:space="preserve">Entrer dans le champ votre adresse courriel UQÀM.</sc:para>
							</op:txt>
						</sp:txt>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Par exemple : prudhomme.steve@courrier.uqam.ca</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_exemple_adresse_email_actuelle.png"/>
							</op:txtRes>
						</sp:txtRes>
					</op:res>
				</sp:warning>
				<sp:info>
					<op:pbTi/>
					<op:res>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Cocher : « Définir Google comme page d'accueil par défaut »</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_coche_page_accueil_defaut.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Sélectionner dans la liste déroulante le : « Canada »</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_liste_deroulante_pays.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Cliquer sur le bouton : « Étapes suivantes »</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_bouton_etapes_suivantes.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Lire les : « Règles de confidentialité et conditions d'utilisation »</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_regles_confidentialite.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Cliquer sur : « J'accepte » si vous acceptez les : « Règles de confidentialité et conditions d'utilisation »</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_bouton_accepte.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Lire de mot de bienvenue de Google</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_mot_bienvenue.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>lft</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Vous êtes maintenant sur la page de bienvenue sur Blogger. Vous devez maintenant confirmer votre profil.</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_page_bienvenue_blogger.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Cliquer sur le bouton : « Créer un profil Google+ »</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_creer_profil_google_plus.png"/>
							</op:txtRes>
						</sp:txtRes>
						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>lft</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Vous êtes maintenant sur la page : « Rejoignez Google + en créant votre profil</sc:para>
									</op:txt>
								</sp:txt>
								<sp:img sc:refUri="/Ressources/DDD9651_rejoignez_google_plus.png"/>
							</op:txtRes>
						</sp:txtRes>
