						<sp:txtRes>
							<op:txtRes>
								<op:txtResM>
									<sp:pos>rgt</sp:pos>
								</op:txtResM>
								<sp:txt>
									<op:txt>
										<sc:para xml:space="preserve">Cliquer sur le lien hypertexte : « Ajoutez une légende »</sc:para>
									</op:txt>
								</sp:txt>
